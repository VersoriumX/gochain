{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        

        {
            "type": "node",
            "request": "attach",
   
            "name": "Docker: Attach to Node",
            "remoteRoot": "/usr/src/app"
        },
        {"name":"Python: Remote Attach","type":"python","request":"attach","connect":{"host":"localhost","port":8333},"pathMappings":[{"localRoot":"${workspaceFolder}","remoteRoot":"."}],"justMyCode":true},
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch add-on",
            "addonPath": "${workspaceFolder}"
        },

        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "live",
            "processId": 0
        },
        {"name":"Python: Module","type":"python","request":"launch","module":"VersoriumX","justMyCode":true},
        {
            "name": "Python: Pyramid Application",
            "type": "python",
            "request": "launch",
            "module": "pyramid.scripts.pserve",
            "args": [
                "${workspaceFolder}\\development.ini"
            ],
            "pyramid": true,
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://192.168.0.9:8334",
            "webRoot": "${edge-workspaceFolder}"
        },
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch index.html",
            "file": "${firefox-workspaceFolder}/index.html"
        },
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "live",
            "processId": 0
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "vcirclex",
            "justMyCode": true
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Docker: Attach to Node",
            "remoteRoot": "/usr/src/app"
        },
        {
            "name": "Python: Remote Attach",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "http://192.168.0.9",
                "port": 8334
            },
            "pathMappings": [
                {
                    "localRoot": "${local-workspaceFolder}",
                    "remoteRoot": "."
                }
            ],
            "justMyCode": true
        },
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/<target-hatdhat>/<project-versoriumx.dll>",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "type": "truffle",
            "request": "launch",
            "name": "Truffle - Debug Transaction",
            "stopOnEntry": false,
            "txHash": "0x",
            "files": [],
            "workingDirectory": "${truffle-workspaceFolder}",
            "providerUrl": "http://127.0.0.1:8334"
        }
    ]
}